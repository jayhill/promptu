using System;
using System.Collections.Generic;
using System.Text;

namespace ZachJohnson.Promptu
{
    using System;

    [global::System.Serializable]
    internal class IncrementingValueCannotBeFoundException : Exception
    {
        public IncrementingValueCannotBeFoundException()
        {
        }

        public IncrementingValueCannotBeFoundException(string message)
            : base(message)
        {
        }

        public IncrementingValueCannotBeFoundException(string message, Exception inner)
            : base(message, inner)
        {
        }

        protected IncrementingValueCannotBeFoundException(
          System.Runtime.Serialization.SerializationInfo info,
          System.Runtime.Serialization.StreamingContext context)
            : base(info, context)
        {
        }
    }
}
