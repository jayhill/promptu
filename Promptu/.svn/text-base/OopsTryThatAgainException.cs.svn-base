using System;
using System.Collections.Generic;
using System.Text;

namespace ZachJohnson.Promptu
{
    [Serializable]
    internal class OopsTryThatAgainException : Exception
    {
        public OopsTryThatAgainException() { }
        public OopsTryThatAgainException(string message) : base(message) { }
        public OopsTryThatAgainException(string message, Exception inner) : base(message, inner) { }
        protected OopsTryThatAgainException(
          System.Runtime.Serialization.SerializationInfo info,
          System.Runtime.Serialization.StreamingContext context)
            : base(info, context) { }
    }
}
