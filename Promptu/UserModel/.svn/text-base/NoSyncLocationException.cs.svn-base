using System;
using System.Collections.Generic;
using System.Text;

namespace ZachJohnson.Promptu.UserModel
{
    [global::System.Serializable]
    internal class NoSyncLocationException : Exception
    {
        public NoSyncLocationException()
        {
        }

        public NoSyncLocationException(string message)
            : base(message)
        {
        }

        public NoSyncLocationException(string message, Exception inner)
            : base(message, inner)
        {
        }

        protected NoSyncLocationException(
          System.Runtime.Serialization.SerializationInfo info,
          System.Runtime.Serialization.StreamingContext context)
            : base(info, context)
        {
        }
    }
}
