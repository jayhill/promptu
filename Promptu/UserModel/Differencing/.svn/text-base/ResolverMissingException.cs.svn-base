using System;
using System.Collections.Generic;
using System.Text;

namespace ZachJohnson.Promptu.UserModel.Differencing
{
    [global::System.Serializable]
    internal class ResolverMissingException : Exception
    {
        public ResolverMissingException()
        {
        }

        public ResolverMissingException(string message)
            : base(message)
        {
        }

        public ResolverMissingException(string message, Exception inner)
            : base(message, inner)
        {
        }

        protected ResolverMissingException(
          System.Runtime.Serialization.SerializationInfo info,
          System.Runtime.Serialization.StreamingContext context)
            : base(info, context)
        {
        }
    }
}
