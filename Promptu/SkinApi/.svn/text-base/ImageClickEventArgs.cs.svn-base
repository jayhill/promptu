using System;
using System.Collections.Generic;
using System.Text;

namespace ZachJohnson.Promptu.SkinApi
{
    public class ImageClickEventArgs : EventArgs
    {
        private string key;

        public ImageClickEventArgs(string key)
        {
            this.key = key;
        }

        public string Key
        {
            get { return this.key; }
        }
    }
}
