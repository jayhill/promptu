<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZachJohnson.Promptu.WpfUI.UIComponents" x:Class="ZachJohnson.Promptu.WpfUI.UIComponents.ProfileConfigPanel" 
             x:ClassModifier="internal"
             mc:Ignorable="d" HorizontalAlignment="Left" >
	<Grid Margin="0,7,0,0">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="*"/>
			<ColumnDefinition Width="*"/>
			<ColumnDefinition Width="*"/>
		</Grid.ColumnDefinitions>
            
		<local:WpfComboInput x:Name="currentProfiles" Margin="0" VerticalAlignment="Center" d:LayoutOverrides="Width" Grid.ColumnSpan="3" Padding="5,4"/>
		<local:WpfButton x:Name="deleteButton" Content="WpfButton" Margin="0,7,7,0" Grid.Row="1" MinWidth="75" MinHeight="23" Padding="6,1"/>
		<local:WpfButton x:Name="newProfileButton" Content="WpfButton" Grid.Column="1" Margin="0,7,7,0" Grid.Row="1" MinWidth="75" MinHeight="23" Padding="6,1,5,1"/>
		<local:WpfButton x:Name="renameButton" Content="WpfButton" Grid.Column="2" Margin="0,7,0,0" Grid.Row="1" MinWidth="75" MinHeight="23" Padding="6,1"/>  
	</Grid>
</UserControl>
