<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZachJohnson.Promptu.WpfUI.UIComponents" x:Class="ZachJohnson.Promptu.WpfUI.UIComponents.PromptuOptionsPanel"
             x:ClassModifier="internal" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<Grid Margin="9,7">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition/>  	
		</Grid.RowDefinitions>
		<TextBlock x:Name="mainInstructions" Margin="5,3,0,11" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Foreground="{DynamicResource MainInstructionsForeground}" FontSize="14.667"/>
        
		<local:OptionsCollectionEditor x:Name="editor" Margin="0" Grid.Row="1"/>
	</Grid>
</UserControl>
